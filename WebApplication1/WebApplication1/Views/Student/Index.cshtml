@model IEnumerable<WebApplication1.Models.Student>

<h2>All Students</h2>

<a asp-action="Create" class="btn btn-primary">Add New Student</a>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Branch</th>
            <th>Gender</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Name</td>
                <td>@student.Branch</td>
                <td>@student.Gender</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@student.Id">Edit</a> |
                    <a asp-action="Delete" asp-route-id="@student.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
